			window.onresize=handle_resize;	
			
			if (self != top) {
				top.location.replace(self.location.href);
			}
			
			function handle_resize()
			{	
				var pathName =window.location.pathname;
				/* var height=window.innerHeight;//Firefox
				if (document.body.clientHeight){
					height=document.body.clientHeight;//IE
				} */
				var height = $(document).height();
				
				var adjustedHeight = parseInt(height- document.getElementById("mainContent").offsetTop-100);
				//do not handle resize if landing page
				if (!document.getElementById("signon") && !document.getElementById("signon2")){
					if(document.getElementById("ifr") ) {
						document.getElementById("ifr").style.height=adjustedHeight+"px";
					} else {
						if ($("#mainContent").height() < adjustedHeight) {
						  document.getElementById("mainContent").style.height= adjustedHeight+"px";
						}
					}
				}
			 
			}
			</script>
